<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.scop.portal.dao.login.AuthMapper">

	<select id="selectUser" parameterType="String" resultType="com.scop.portal.domain.Member">
		SELECT id, pw
		  FROM Member
		WHERE id = #{userId}
	</select>

</mapper>